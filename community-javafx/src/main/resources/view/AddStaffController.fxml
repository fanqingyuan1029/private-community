<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="601.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.fqy.controller.AddStaffController">
   <Separator layoutY="70.0" prefHeight="2.0" prefWidth="600.0"/>
   <Label fx:id="titleLabel" layoutX="252.0" layoutY="34.0" text="标题标题">
      <font>
         <Font name="KaiTi" size="24.0"/>
      </font>
   </Label>
   <AnchorPane fx:id="addPane" layoutY="60.0" prefHeight="514.0" prefWidth="600.0">
      <TextField fx:id="username" layoutX="206.0" layoutY="40.0" promptText="用户名">
         <font>
            <Font size="14.0"/>
         </font>
      </TextField>
      <TextField fx:id="password" layoutX="206.0" layoutY="120.0" promptText="密码">
         <font>
            <Font size="14.0"/>
         </font>
      </TextField>
      <TextField fx:id="name" layoutX="206.0" layoutY="200.0" promptText="姓名">
         <font>
            <Font size="14.0"/>
         </font>
      </TextField>
      <TextField fx:id="type" layoutX="206.0" layoutY="280.0" promptText="职称">
         <font>
            <Font size="14.0"/>
         </font>
      </TextField>
      <TextField fx:id="phone" layoutX="206.0" layoutY="360.0" promptText="联系电话">
         <font>
            <Font size="14.0"/>
         </font>
      </TextField>
   </AnchorPane>
   <Button fx:id="titleButton" layoutX="260.0" layoutY="497.0" mnemonicParsing="false" onAction="#titleButton"
           text="标题">
      <font>
         <Font name="KaiTi" size="24.0"/>
      </font>
   </Button>
</AnchorPane>
